{
  "name": "watermelondb-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "React Native Expo App with WatermelonDB - Monorepo Structure",
  "scripts": {
    "mobile": "cd mobile && npm start",
    "mobile:android": "cd mobile && npm run android",
    "mobile:ios": "cd mobile && npm run ios",
    "mobile:web": "cd mobile && npm run web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "d:u": "docker compose --env-file .env.development -f compose.dev.yml up",
    "d:ut": "docker compose --env-file .env.test -f compose.test.yml up",
    "d:up": "docker compose -f compose.yml up",
    "d:us": "docker compose -f ./stage/compose.stage.yml up",
    "d:usssl": "docker compose -f ./stage/traefik/compose.yml up",
    "d:uppma": "docker compose -f compose.stage.phpmyadmin.yml up",
    "d:te2e": "docker exec backend npm run test:e2e",
    "d:te2edebug": "docker exec backend npm run test:e2edebug",
    "d:pm": "docker exec backend npm run prisma:migrate",
    "d:pf": "docker exec backend npm run prisma:format"
  },
  "keywords": [
    "watermelondb",
    "expo",
    "react-native",
    "monorepo"
  ],
  "author": "",
  "license": "MIT"
}